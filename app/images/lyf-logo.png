    border-color: #EEE;
    padding: 4;
    margin: 8 0;
}

.profile-email {
    color: #666;
    font-size: 16;
    text-align: center;
}

.section-container {
    background-color: white;
    margin-top: 16;
    padding: 24;
}

.section-title {
    color: #333;
    font-size: 20;
    font-weight: bold;
    margin-bottom: 16;
}

.preferences-grid {
    margin: 8 0;
}

.preference-label {
    color: #333;
    font-size: 16;
    vertical-align: center;
}

.preference-switch {
    color: #FF6B00;
    vertical-align: center;
}

.account-button {
    margin: 8 0;
